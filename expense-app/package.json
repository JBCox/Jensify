{
  "name": "expense-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:analyze": "ng build --named-chunks --stats-json && echo 'Run: npx webpack-bundle-analyzer dist/expense-app/stats.json'",
    "build:stats": "ng build --stats-json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --browsers=ChromeHeadless --watch=false",
    "test:coverage": "ng test --code-coverage --browsers=ChromeHeadless --watch=false",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "type-check": "ng build --configuration development --no-emit",
    "serve:prod": "ng serve --configuration production",
    "lint": "ng lint"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.0",
    "@angular/cdk": "^20.2.12",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.12",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/service-worker": "^20.3.11",
    "@googlemaps/js-api-loader": "^2.0.2",
    "@supabase/supabase-js": "^2.83.0",
    "@types/uuid": "^10.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.10",
    "@angular/cli": "^20.3.10",
    "@angular/compiler-cli": "^20.3.0",
    "@types/google.maps": "^3.58.1",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.6.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.3"
  }
}